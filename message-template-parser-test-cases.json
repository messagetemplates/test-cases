[
  {
    "input": "Hello, world!",
    "output": [ { "text": "Hello, world!" } ],
    "title": "Single Text"
  },
  {
    "input": "{Name}",
    "output": [ { "property": "Name" } ],
    "title": "Single property"
  },
  {
    "input": "{0}",
    "output": [
      {
        "property": "0",
        "positional": true
      }
    ],
    "title": "Single positional item"
  },
  {
    "input": "Hello, {Name}!",
    "output": [
      { "text": "Hello, " },
      { "property": "Name" },
      { "text": "!" }
    ],
    "title": "Combined text and property"
  }

]
